<?xml version="1.0" encoding="utf-8"?>
<project name="functional.tiapkss.test.suite">
  <description>
    tiapkss test suite in ohs team.

    Edit this file to control feature test suite

  </description>

  <!-- ADDED-BY-SEED location of this file -->
  <dirname property="functional.tiapkss.test.suite.dir" 
           file="${ant.file.functional.tiapkss.test.suite}" />

  <!-- ADDED-BY-SEED Id of test suite -->
  <property name="suiteid" value="functional.tiapkss.suite" />

  <!-- ADDED-BY-SEED Id of property set that is passed to test files -->
  <property name="suite.propertysetid" 
            value="functional.tiapkss.suite.propertysetid"/>

  <!-- Importing feature suite files -->
  <import file="${functional.tiapkss.test.suite.dir}/single1/single1-test-suite.xml"/>
  <import file="${functional.tiapkss.test.suite.dir}/single2/single2-test-suite.xml"/>
  <import file="${functional.tiapkss.test.suite.dir}/multi1/multi1-test-suite.xml"/>
  <import file="${functional.tiapkss.test.suite.dir}/exception/exception-test-suite.xml"/>


  <!-- ADDED-BY-SEED team test xml files -->
  <projects id="functional.tiapkss.suite">
     <projects refid="functional.tiapkss.single1.suite"/>
     <projects refid="functional.tiapkss.single2.suite"/>
     <projects refid="functional.tiapkss.multi1.suite"/>
     <projects refid="functional.tiapkss.exception.suite"/>

  </projects>

  <!-- ADDED-BY-SEED Properties that are passed to test xml files that makeup
                     above suite -->
  <propertyset id="functional.tiapkss.suite.propertysetid">
    <propertyref builtin="commandline" />
  </propertyset>

</project>
