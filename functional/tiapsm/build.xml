  <!-- ADDED-BY-SEED Import tiapsm test suite -->
  <import file="${tiapsm.dir}/tiapsm-test-suite.xml" />

  <!-- ADDED-BY-SEED Import common test directory targets -->
  <import file="${tl.cafe-dir.file}" />

