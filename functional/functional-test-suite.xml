<?xml version="1.0"?>
<project name="functional.suite">
  <description>
   Functional test suite.

    NOTE: Edit this file to add a new test suite that are part of functional run.
  </description>

  <!-- ADDED-BY-SEED Location of this file -->
  <dirname property="functional.suite.dir"
           file="${ant.file.functional.suite}" />

  <!-- ADDED-BY-SEED Id of test suite -->
  <property name="suiteid" value="functional.suite" />

  <!-- ADDED-BY-SEED Id of property set that is passed to test files -->
  <property name="suite.propertysetid" value="functional.suite.propertysetid"/>

  <!--
    ADDED-BY-SEED

    ACTION import all team suite files. e.g.
  -->
  <import file="${functional.suite.dir}/tiapsm/tiapsm-test-suite.xml"/>
  <import file="${functional.suite.dir}/tiapmoi/tiapmoi-test-suite.xml"/>
  <import file="${functional.suite.dir}/tiapcp/tiapcp-test-suite.xml"/>
  <import file="${functional.suite.dir}/tiapkss/tiapkss-test-suite.xml"/>
  <import file="${functional.suite.dir}/tiapwt/tiapwt-test-suite.xml"/>
  <import file="${functional.suite.dir}/tiapsh/tiapsh-test-suite.xml"/>
  <import file="${functional.suite.dir}/tiapwl/tiapwl-test-suite.xml"/>


  <!-- ADDED-BY-SEED functional test suite -->
  <projects id="functional.suite">
    <projects refid="functional.tiapsm.suite"/>
    <projects refid="functional.tiapmoi.suite"/>
    <projects refid="functional.tiapcp.suite"/>
    <projects refid="functional.tiapkss.suite"/>
    <projects refid="functional.tiapwt.suite"/>
    <projects refid="functional.tiapsh.suite"/>
    <projects refid="functional.tiapwl.suite"/>

  </projects>

  <!-- ADDED-BY-SEED Properties that are passed to test xml files that makeup
                     above suite -->
  <propertyset id="functional.suite.propertysetid">
    <propertyref builtin="commandline" />
  </propertyset>

</project>
