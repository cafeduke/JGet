<?xml version="1.0" encoding="utf-8"?>
<project name="functional.tiapfc.test.suite">
  <description>
    tiapfc test suite in ohs team.

    Edit this file to control feature test suite

  </description>

  <!-- ADDED-BY-SEED location of this file -->
  <dirname property="functional.tiapfc.test.suite.dir" 
           file="${ant.file.functional.tiapfc.test.suite}" />

  <!-- ADDED-BY-SEED Id of test suite -->
  <property name="suiteid" value="functional.tiapfc.suite" />

  <!-- ADDED-BY-SEED Id of property set that is passed to test files -->
  <property name="suite.propertysetid" 
            value="functional.tiapfc.suite.propertysetid"/>

  <!-- Importing feature suite files -->
  <import file="${functional.tiapfc.test.suite.dir}/basic/basic-test-suite.xml"/>

  <!-- ADDED-BY-SEED team test xml files -->
  <projects id="functional.tiapfc.suite">
     <projects refid="functional.tiapfc.basic.suite"/>

  </projects>

  <!-- ADDED-BY-SEED Properties that are passed to test xml files that makeup
                     above suite -->
  <propertyset id="functional.tiapfc.suite.propertysetid">
    <propertyref builtin="commandline" />
  </propertyset>

</project>
