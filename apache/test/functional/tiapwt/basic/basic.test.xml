    <target name="01">
    <description>
    <!-- @testlogic.group type="category" values="mats" -->
    Incremental response data stress test, multi-thread model
    </description>
    <antcall target="pingOHSJava">
    <param name="out.log" value="tiapwt_basic_01.log"/>
    <param name="ohs.name" value="ohs1"/>
    <param name="url" value="/waiter/servlet/ServletStressMulti?mode=1&amp;iterations=10&amp;initial=100&amp;increment=10000"/>
	<param name="file1" value="tiapwt_basic_01.txt"/>
	</antcall>

		<condition property="fail-res">
            <resourcecontains resource="${twork.dir}/tiapwt_basic_01.log" substring="200"/>
        </condition>
    <fail message="Response code is not 200... failing." unless="fail-res"/>

		<condition property="fail-test">
			<resourcecontains resource="${twork.dir}/tiapwt_basic_01.txt" substring="test PASSED."/>
		</condition>
	<fail message="test PASSED. not found... failing" unless="fail-test"/>
    </target>

